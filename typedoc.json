{
    "name": "Aberdeen",
    "readme": "README.md",
    "entryPoints": ["src/*.ts"],
    "projectDocuments": ["docs/*.md"],
    "excludeInternal": true,
    "excludePrivate": true,
    "includeVersion": true,
    "router": "structure-dir",
    "highlightLanguages": [
        "typescript",
        "javascript",
        "html"
    ],
    "searchInComments": true,
    "navigationLinks": {
        "What's this?": "https://aberdeenjs.org/",
        "Tutorial": "https://aberdeenjs.org/Tutorial/",
        "GitHub": "https://github.com/vanviegen/aberdeen"
    },
    "hostedBaseUrl": "https://aberdeenjs.org/",
    "useFirstParagraphOfCommentAsSummary": true,
    "validation": {
        "notDocumented": true
    },
    "treatWarningsAsErrors": true,
    "out": "./dist-docs",
    "customJs": "docs/live-code.js"
}